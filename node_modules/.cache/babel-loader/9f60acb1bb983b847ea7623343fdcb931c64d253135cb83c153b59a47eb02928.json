ed () {\r\n    this.user = JSON.parse(localStorage.getItem('userinfo'))\r\n    axios.get('http://localhost:3000/messages').then(res => {\r\n      this.messages = res.data\r\n    })\r\n  },\r\n  components: {\r\n    // Barrage\r\n  },\r\n  methods: {\r\n    showButton () {\r\n      this.isShowButton = true\r\n    },\r\n\r\n    async submitbarrage () {\r\n      if (!this.islogin) {\r\n        Swal.fire({\r\n          title: 'ÂìéÂëÄ~',\r\n          text: '‰Ω†ËøòÊ≤°ÁôªÂΩïÂë¢ÔºåÁ¨®Ëõã~ ü•∫',\r\n          icon: 'warning',\r\n          confirmButtonText: 'È©¨‰∏äÂéªÁôªÂΩï üíñ',\r\n          background: '#fff0f6', // ÂèØÁà±Á≤âËâ≤\r\n          customClass: {\r\n            popup: 'cute-popup'\r\n          },\r\n          preConfirm: () => {\r\n            this.$router.push({ path: '/login' }) // Ë∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µÈù¢\r\n          }\r\n        })\r\n        return\r\n      }\r\n      if (!this.content.trim()) {\r\n        return Message.warning('Ê≤°‰∏úË•øËøòÂèëü§®')\r\n      }\r\n      if (this.isSubmitting) return\r\n      this.isSubmitting = true\r\n      // Â∞ÜÁî®Êà∑ËæìÂÖ•ÁöÑÂÜÖÂÆπÂèëÈÄÅÁªô WebSocket ÊúçÂä°Âô®\r\n      const newMessage = {\r\n        userId: this.user.userId,\r\n        text: this.content,\r\n        createdAt: new Date().toISOString()\r\n      }\r\n      const response = await axios.post('http://localhost:3000/messages', newMessage)\r\n      this.messages = response.data\r\n      console.log(this.messages)\r\n\r\n      this.content = ''\r\n      setTimeout(() => {\r\n        this.isSubmitting = false\r\n      }, 1000)\r\n    }\r\n\r\n  },\r\n  computed: {\r\n    ...mapGetters('user', ['getUserInfo']),\r\n    islogin () {\r\n      console.log('Vuex ÈáåÁöÑ getUserInfo:', this.getUserInfo)\r\n      return !!(this.getUserInfo && this.getUserInfo.userId)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.remark {\r\n  position: relative;\r\n  top: -280px;\r\n}\r\nh1 {\r\n  font-family: Arial, Helvetica, sans-serif;\r\n  text-align: center;\r\n  colo