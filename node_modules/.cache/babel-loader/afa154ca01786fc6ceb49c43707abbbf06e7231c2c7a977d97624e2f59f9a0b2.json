content.trim()) {\n        return Message.warning('ËæìÂÖ•‰∏çËÉΩ‰∏∫Á©∫');\n      }\n\n      // Èò≤Ê≠¢ÈáçÂ§çÊèê‰∫§\n      if (this.isSubmitting) return;\n      this.isSubmitting = true;\n\n      // ÂèëÈÄÅÂºπÂπïÂÜÖÂÆπÂà∞ÊúçÂä°Âô®\n      this.socket.send(this.content);\n\n      // Ê∏ÖÁ©∫ËæìÂÖ•Ê°Ü\n      this.content = '';\n\n      // 5ÁßíÂêéÊÅ¢Â§çÊèê‰∫§ÊåâÈíÆ\n      setTimeout(() => {\n        this.isSubmitting = false;\n      }, 1000);\n    }\n  },\n  computed: {\n    ...mapGetters('user', ['getUserInfo']),\n    islogin() {\n      console.log('Vuex ÈáåÁöÑ getUserInfo:', this.getUserInfo);\n      return !!(this.getUserInfo && this.getUserInfo.userId);\n    }\n  }\n};","map":{"version":3,"names":["Barrage","mapGetters","Message","Swal","name","data","content","isSubmitting","isShowButton","messages","socket","created","initWebSocket","components","methods","showButton","WebSocket","onopen","console","log","onmessage","event","text","push","onerror","error","onclose","submitbarrage","islogin","fire","title","icon","confirmButtonText","background","customClass","popup","preConfirm","$router","path","trim","warning","send","setTimeout","computed","getUserInfo","userId"],"sources":["src/views/remark/index.vue"],"sourcesContent":["<template>\r\n  <div class=\"remark\">\r\n    <h1>Ê†ëÊ¥û</h1>\r\n    <input type=\"text\" placeholder=\"Áïô‰∏ãÁÇπ‰ªÄ‰πàÂï¶~\" class=\"dreamBoard\" @click=\"showButton\" v-model=\"content\">\r\n    <button class=\"pushRemark\" v-if=\"isShowButton\" @click=\"submitbarrage\" :disabled=\"isSubmitting\">{{ isSubmitting ? 'ÂèëÂ∞Ñ‰∏≠...' : 'ÂèëÂ∞Ñ' }}</button>\r\n    <Barrage :messages=\"messages\"></Barrage>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Barrage from '@/components/Barrage.vue'\r\nimport { mapGetters } from 'vuex'\r\nimport { Message } from 'element-ui'\r\nimport Swal from 'sweetalert2'\r\n\r\nexport default {\r\n  name: 'remarkIndex',\r\n  data () {\r\n    return {\r\n      content: '', // Áî®Êà∑ËæìÂÖ•ÁöÑÂÜÖÂÆπ\r\n      isSubmitting: false, // Êèê‰∫§Áä∂ÊÄÅ\r\n      isShowButton: false, // ÊòØÂê¶ÊòæÁ§∫ÊåâÈíÆ\r\n      messages: [], // Â≠òÂÇ®ÂºπÂπïÁöÑÊï∞ÁªÑ\r\n      socket: null // WebSocket ËøûÊé•\r\n    }\r\n  },\r\n  created () {\r\n    this.initWebSocket()\r\n  },\r\n  components: {\r\n    Barrage\r\n  },\r\n  methods: {\r\n    // ÁÇπÂáªËæìÂÖ•Ê°ÜÊó∂ÊòæÁ§∫ÊåâÈíÆ\r\n    showButton () {\r\n      this.isShowButton = true\r\n    },\r\n\r\n    // ÂàùÂßãÂåñ WebSocket\r\n    initWebSocket () {\r\n      this.socket = new WebSocket('ws://localhost:8080')\r\n\r\n      this.socket.onopen = () => {\r\n        console.log('ËøûÊé• WebSocket ÊúçÂä°Âô®ÊàêÂäü')\r\n      }\r\n\r\n      this.socket.onmessage = (event) => {\r\n        // Êé•Êî∂Âà∞ÊúçÂä°Âô®ËøîÂõûÁöÑÂºπÂπï\r\n        const text = event.data\r\n        this.messages.push(text) // Êõ¥Êñ∞ÂºπÂπïÂàóË°®\r\n      }\r\n\r\n      this.socket.onerror = (error) => {\r\n        console.error('WebSocket ÈîôËØØ:', error)\r\n      }\r\n\r\n      this.socket.onclose = () => {\r\n        console.log('WebSocket ËøûÊé•Â∑≤ÂÖ≥Èó≠')\r\n      }\r\n    },\r\n\r\n    // Êèê‰∫§ÂºπÂπï\r\n    async submitbarrage () {\r\n      if (!this.islogin) {\r\n        Swal.fire({\r\n          title: 'ÂìéÂëÄ~',\r\n          text: '‰Ω†ËøòÊ≤°ÁôªÂΩïÂë¢ÔºåÁ¨®Ëõã~ ü•∫',\r\n          icon: 'warning',\r\n          confirmButtonText: 'È©¨‰∏äÂéªÁôªÂΩï üíñ',\r\n          background: '#fff0f6', // ÂèØÁà±Á≤âËâ≤\r\n          customClass: {\r\n            popup: 'cute-popup'\r\n          },\r\n          preConfirm: () => {\r\n            this.$router.push({ path: '/login' }) // Ë∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µÈù¢\r\n          }\r\n        })\r\n\r\n        return\r\n      }\r\n\r\n      // ËæìÂÖ•‰∏çËÉΩ‰∏∫Á©∫\r\n      if (!this.content.trim()) {\r\n        return Message.warning('ËæìÂÖ•‰∏çËÉΩ‰∏∫Á©∫')\r\n      }\r\n\r\n      // Èò≤Ê≠¢ÈáçÂ§çÊèê‰∫§\r\n      if (this.isSubmitting) return\r\n      this.isSubmitting = true\r\n\r\n      // ÂèëÈÄÅÂºπÂπïÂÜÖÂÆπÂà∞ÊúçÂä°Âô®\r\n      this.socket.send(this.content)\r\n\r\n      // Ê∏ÖÁ©∫ËæìÂÖ•Ê°Ü\r\n      this.content = ''\r\n\r\n      // 5ÁßíÂêéÊÅ¢Â§çÊèê‰∫§ÊåâÈíÆ\r\n      setTimeout(() => {\r\n        this.isSubmitting = false\r\n      }, 1000)\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters('user', ['getUserInfo']),\r\n    islogin () {\r\n      console.log('Vuex ÈáåÁöÑ getUserInfo:', this.getUserInfo)\r\n      return !!(this.getUserInfo && this.getUserInfo.userId)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.remark {\r\n  position: relative;\r\n  top: -280px;\r\n}\r\nh1 {\r\n  font-family: Arial, Helvetica, sans-serif;\r\n  text-align: center;\r\n  color: white;\r\n}\r\n.dreamBoard {\r\n  width: 200px;\r\n  height: 30px;\r\n  border-radius: 15px;\r\n  box-shadow: 10px white;\r\n  background-color: transparent;\r\n  color: white;\r\n}\r\n\r\n.dreamBoard::placeholder{\r\n  text-indent: 10px;\r\n  color: white;\r\n  opacity: 0.8;\r\n}\r\n.pushRemark {\r\n    width: 80px;\r\n    height: 30px;\r\n    color: white;\r\n    border-radius: 15px;\r\n    cursor: pointer;\r\n    background-color: transparent;\r\n    margin-left: 5px;\r\n    border-radius: 10px;\r\n    font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\r\n    transition: transform 0.3;\r\n}\r\n</style>\r\n"],"mappings":";AAUA,OAAAA,OAAA;AACA,SAAAC,UAAA;AACA,SAAAC,OAAA;AACA,OAAAC,IAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MAAA;MACAC,YAAA;MAAA;MACAC,YAAA;MAAA;MACAC,QAAA;MAAA;MACAC,MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,aAAA;EACA;EACAC,UAAA;IACAb;EACA;EACAc,OAAA;IACA;IACAC,WAAA;MACA,KAAAP,YAAA;IACA;IAEA;IACAI,cAAA;MACA,KAAAF,MAAA,OAAAM,SAAA;MAEA,KAAAN,MAAA,CAAAO,MAAA;QACAC,OAAA,CAAAC,GAAA;MACA;MAEA,KAAAT,MAAA,CAAAU,SAAA,GAAAC,KAAA;QACA;QACA,MAAAC,IAAA,GAAAD,KAAA,CAAAhB,IAAA;QACA,KAAAI,QAAA,CAAAc,IAAA,CAAAD,IAAA;MACA;MAEA,KAAAZ,MAAA,CAAAc,OAAA,GAAAC,KAAA;QACAP,OAAA,CAAAO,KAAA,kBAAAA,KAAA;MACA;MAEA,KAAAf,MAAA,CAAAgB,OAAA;QACAR,OAAA,CAAAC,GAAA;MACA;IACA;IAEA;IACA,MAAAQ,cAAA;MACA,UAAAC,OAAA;QACAzB,IAAA,CAAA0B,IAAA;UACAC,KAAA;UACAR,IAAA;UACAS,IAAA;UACAC,i