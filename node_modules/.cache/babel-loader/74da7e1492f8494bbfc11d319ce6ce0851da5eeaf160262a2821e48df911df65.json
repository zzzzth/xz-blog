ticle = article\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.article-page {\r\n  text-align: center;\r\n  padding: 20px;\r\n}\r\n\r\n.articles-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  gap: 20px;\r\n}\r\n\r\n.article-card {\r\n  background: white;\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\r\n  width: 300px;\r\n  cursor: pointer;\r\n}\r\n\r\n.article-card h2 {\r\n  font-size: 20px;\r\n  color: #333;\r\n}\r\n\r\n.article-card p {\r\n  font-size: 14px;\r\n  color: #666;\r\n}\r\n\r\n.article-card button {\r\n  margin-top: 10px;\r\n  padding: 5px 10px;\r\n  border: none;\r\n  background: #007bff;\r\n  color: white;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n}\r\n\r\n.article-content {\r\n  background: white;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  text-align: left;\r\n}\r\n\r\n.article-content button {\r\n  margin-bottom: 10px;\r\n  background: #ff4d4f;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  color: white;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n"],"mappings":"AAwBA;EACAA,IAAA;EACAC,eAAA;EACAC,KAAA;IACA;MACAC,WAAA,GACA;QAAAC,EAAA;QAAAC,KAAA;QAAAC,WAAA;QAAAC,OAAA;MAAA,GACA;QAAAH,EAAA;QAAAC,KAAA;QAAAC,WAAA;QAAAC,OAAA;MAAA;IAEA;EACA;EACAC,OAAA;IACAC,cAAAC,OAAA;MACA,KAAAT,eAAA,GAAAS,OAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}